{
	"name": "@jonales/n8n-nodes-oracle-database",
	"type": "commonjs",
	"version": "1.0.1-rc.11",
	"description": "Oracle Database node com recursos avançados para cargas pesadas e Oracle 19c+. Suporte completo para PL/SQL, bulk operations, connection pooling e Oracle Cloud Infrastructure com instalação automática do cliente Oracle.",
	"keywords": [
		"n8n-community-node-package",
		"oracle"
	],
	"license": "MIT",
	"homepage": "https://github.com/jonales/n8n-nodes-oracle-database",
	"author": {
		"name": "Jônatas Meireles Sousa Vieira",
		"email": "jonatas.mei@outlook.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jonales/n8n-nodes-oracle-database.git"
	},
	"main": "index.js",
	"engines": {
		"node": ">=18.18.0",
		"npm": ">=9.0.0"
	},
	"os": [
		"darwin",
		"linux",
		"win32"
	],
	"cpu": [
		"x64",
		"arm64"
	],
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier credentials nodes --write",
		"lint": "tslint -p tsconfig.json -c tslint.json",
		"lintfix": "tslint --fix -p tsconfig.json -c tslint.json",
		"prepublishOnly": "npm run build",
		"setup": "node script/setup-oracle.js",
		"setup:force": "node script/setup-oracle.js --force",
		"install-oracle": "node script/oracle-installer.js",
		"test-connection": "node script/test-connection.js",
		"postinstall": "npm run setup"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"files": [
		"dist",
		"script",
		"README.md",
		"LICENSE"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/Oracle.credentials.js"
		],
		"nodes": [
			"dist/nodes/Oracle/OracleDatabase.node.js",
			"dist/nodes/Oracle/OracleDatabaseAdvanced.node.js",
			"dist/nodes/Oracle/OracleVectorStore.node.js",
			"dist/nodes/Oracle/ChatMemory.node.js"
		]
	},
	"devDependencies": {
		"@types/express": "^4.17.6",
		"@types/oracledb": "^6.9.1",
		"@types/request-promise-native": "~1.0.21",
		"@typescript-eslint/parser": "^5.29.0",
		"eslint-plugin-n8n-nodes-base": "^1.5.4",
		"gulp": "^4.0.2",
		"n8n-core": "^1.106.2",
		"n8n-workflow": "^1.105.0",
		"prettier": "^2.7.1",
		"tslint": "^6.1.2",
		"typescript": "~4.6.0"
	},
	"dependencies": {
		"oracledb": "^6.9.0"
	}
}